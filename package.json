{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "description": "A platform where users can order for food.",
  "main": "app.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 10000 test/appTest.js --compilers js:babel-register --exit || true",
    "coveralls": "nyc npm test && nyc report --reporter=lcov --reporter=text --reporter=lcovonly || coveralls",
    "start": "node dist/app.js",
    "build": "babel src -d dist"
  },
  "nyc": {
    "exclude": [
      "**/controllers/**",
      "**/model/**",
      "**/UI/**",
      "client"
    ]
  },
  "reporter": [
    "lcov",
    "text"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbols/fast-food-fast.git"
  },
  "keywords": [
    "food",
    "orders"
  ],
  "author": "olagunju gbolahan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gbols/fast-food-fast/issues"
  },
  "homepage": "https://github.com/gbols/fast-food-fast#readme",
  "dependencies": {
    "arrify": "^1.0.1",
    "bcrypt": "^3.0.0",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.3",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  }
}
